# Domain Configuration
DOMAIN=localhost
CLOUDFLARE_TOKEN=your-cloudflare-api-token

# Admin Dashboard
ADMIN_PASSWORD=rootpassword

# LibSQL Configuration (automatically set in Docker)
LIBSQL_URL=http://libsql:8080
LIBSQL_AUTH_TOKEN=

# Valkey (Redis) Configuration (automatically set in Docker)
VALKEY_URL=redis://valkey:6379

# SeaweedFS (S3) Configuration (automatically set in Docker)
S3_ENDPOINT=http://seaweedfs:8333
S3_ACCESS_KEY=admin
S3_SECRET_KEY=admin
S3_DEFAULT_BUCKET=v8box
S3_PROJECTS_BUCKET=v8box-projects
S3_FUNCTIONS_BUCKET=v8box-functions
S3_REGION=us-east-1

# Server Configuration
PORT=8000

